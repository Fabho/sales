<table style="width:100%; margin-bottom:25px">
  <tr>
    <td>
      <div class="ListItemDiv">
        <div class="HeaderListDiv">Barang</div>
        <div class="ItemListContentDiv">
          <div class="ActionButtonDiv">
            <table width="100%">
              <tr>
                <td>
                  <table>
                    <tr>
                      <td>
                        <div class="menuLeft" id="createEstimation">Buat Estimasi</div>
                      </td>
                      <td>
                        <div class="menuLeft" id="createinvoice">Buat Faktur Penjualan</div>
                      </td>
                      <td>
                        <div class="menuLeft" id="deleteItem">
                          <div class="floatLeft">Hapus</div>
                          <div class="iconHapus"></div>
                        </div>
                      </td>
                    </tr>
                  </table>
                </td>
                <td style="float: right;">
                  <table>
                    <tr>
                      <td>
                        <div style="color:grey;">Tampilkan :</div>
                      </td>
                      <td>
                        <div>
                          <div class=" floatRight menuLeft">
                            <div class="floatRight">Aktif</div>
                            <div class="imgFilter floatRight"></div>
                          </div>
                        </div>
                      </td>
                      <td>
                        <div class=" floatLeft">
                          <div class="DivSearch">
                            <div class="imgSearch">
                              <input type="input" id="searchItem" placeholder="Cari"/>
                            </div>
                          </div>
                          <div class="searchOption">
                            <table id="autoCompleteItems" style="width:100%"></table>
                          </div>
                        </div>
                      </td>
                    </tr>
                  </table>
                </td>
              </tr>
            </table>
          </div>
          <table class="ItemList">
            <thead>
              <tr>
                <th class="headerList headerMark">
                  <input type="checkbox" id="checkBoxAllList" value="" />
                </th>
                <th class="headerList headerEvent"></th>
                <th class="headerList headerItems" >Nama Barang</th>
                <th class="headerList headerPrice" >Harga</th>
              </tr>
            </thead>
            <tbody class="BodyDataItem">

            </tbody>
          </table>
        </div>
        <div class ='Pagging'>
          <div class ='DivPrev'>
            <img class ='first'></img>
            <img class ='prev'></img>
          </div>
          <div class ="disablePrev" style="display:none;">
            <img class ='firstDisable'></img>
            <img class ='prevDisable'></img>
          </div>
          <div class ='DivPage'>
            Page <input type='text' id='idInputPage' style="width:20px; height:14px"/> of <span id="totalPage"></span>
          </div>
          <div class='DivNext'>
            <img class ='next'></img>
            <img class ='last'></img>
          </div>
          <div class ="disableNext" style="display:none;">
            <img class ='nextDisable'></img>
            <img class ='lastDisable'></img>
          </div>
          <div class='DivlimitData'>
            <select class="limit" id="limitData" style="width:50px; height:21px">
              <option value="10" selected="selected"> 10 </option>
              <option value="20"> 20 </option>
              <option value="30"> 30 </option>
              <option value="40"> 40 </option>
            </select>
          </div>
        </div>
      </div>
    </td>
    <td style="width:20%;vertical-align:top; padding:30px 0 0 10px;">
      <button type="button" id="gotoCreateItem" >
        <img style="width:15px;height:15px;vertical-align:top;" src="/sales/controllers/items/list/images/add.png"/>
        Tambah Barang
      </button>
    </td>
  </tr>
</table>